<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.blog.mapper.CmsArticleMapper">
    <select id="findCmsArticleList" resultType="com.example.blog.domain.entity.CmsArticle">
        select *
        from cms_article
        where delete_flag = 0
    </select>
</mapper>
